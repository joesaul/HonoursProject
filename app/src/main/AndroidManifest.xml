        <activity
            android:name=".Thursday"
            android:label="@string/thursday"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Wednesday"
            android:label="@string/wednesday"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Tuesday"
            android:label="@string/tuesday"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Monday"
            android:label="@string/monday"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TaylorCourt"
            android:label="@string/taylorcourt"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Westfield"
