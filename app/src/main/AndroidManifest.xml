            android:label="@string/courtyard"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CampusMap"
            android:label="@string/campusmap"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Parking"
            android:label="@string/parking"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Schedule"
            android:label="@string/schedule"
            android:parentActivityName=".MainActivity" />
        <activity
