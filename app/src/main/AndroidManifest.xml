        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <uses-library android:name="org.apache.http.legacy" android:required="false" />
        <activity android:name=".ComputerScience"></activity>
        <activity
            android:name=".Sunday"
            android:label="@string/sunday"
